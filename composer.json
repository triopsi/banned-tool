{
    "name": "triopsi/banned-tool",
    "description": "Ban tool plugin for CakePHP. A quick way to ban IP addresses.",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "4.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "BannedTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BannedTool\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
